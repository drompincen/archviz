    "flows": [
        {
            "id": "legacy-phone",
            "name": "Legacy — Phone Appointment",
            "sequence": [
                { "from": "pet-owner", "to": "receptionist", "text": "\"Hi, Bella hasn't been eating — can I get an appointment?\" (phone call)", "status": "ready" },
                { "from": "receptionist", "to": "wall-calendar", "text": "Flip through wall calendar: \"Let me see what's open...\"", "status": "wip" },
                { "from": "wall-calendar", "to": "receptionist", "text": "\"Thursday at 2pm is open\" — scribbles in pencil", "status": "wip", "popUp": { "type": "alert", "msg": "4.5 min phone hold time — 30% of callers hang up before booking" } },
                { "from": "receptionist", "to": "paper-charts", "text": "Pull Bella's paper chart from filing cabinet", "status": "wip" },
                { "from": "paper-charts", "to": "vet-doctor", "text": "Leave chart on Dr. Chen's desk for Thursday", "status": "wip" },
                { "from": "vet-doctor", "to": "exam-room", "text": "Thursday: examine Bella — temp, weight, palpation", "status": "ready" },
                { "from": "vet-doctor", "to": "paper-rx", "text": "Handwrite prescription for anti-nausea meds", "status": "wip", "popUp": { "type": "alert", "msg": "No digital record — illegible handwriting is a medication error risk" } }
            ]
        },
        {
            "id": "legacy-walkin",
            "name": "Legacy — Walk-in Emergency",
            "sequence": [
                { "from": "pet-owner", "to": "receptionist", "text": "\"My dog Max cut his paw — it's bleeding!\" (walks in, panicked)", "status": "ready" },
                { "from": "receptionist", "to": "paper-charts", "text": "Scramble to find Max's chart — is it under M? under owner name?", "status": "wip", "popUp": { "type": "issue", "msg": "Chart not found — misfiled 2 weeks ago, still missing" } },
                { "from": "receptionist", "to": "vet-doctor", "text": "\"Dr. Patel, emergency walk-in — no chart, new intake form\"", "status": "wip" },
                { "from": "vet-doctor", "to": "exam-room", "text": "Clean wound, assess depth, decide on stitches", "status": "ready" },
                { "from": "vet-doctor", "to": "paper-rx", "text": "Write Rx: antibiotics + pain meds, handwritten", "status": "wip" },
                { "from": "paper-rx", "to": "pharmacy", "text": "Rush Rx to pharmacy — \"can you read this?\"", "status": "wip", "popUp": { "type": "issue", "msg": "Pharmacy tech can't read dosage — calls vet to clarify, 10 min delay" } }
            ]
        },
        {
            "id": "online-booking",
            "name": "Phase 1 — Online Booking",
            "sequence": [
                { "from": "pet-owner", "to": "online-portal", "text": "Book appointment: \"Bella — wellness check, Tuesday 10am\"", "status": "ready", "phase": "digital" },
                { "from": "online-portal", "to": "appt-svc", "text": "Validate slot availability in real-time", "status": "ready", "phase": "digital" },
                { "from": "appt-svc", "to": "emr-db", "text": "Create appointment record + pull Bella's digital chart", "status": "ready", "phase": "digital" },
                { "from": "appt-svc", "to": "appt-queue", "text": "Queue confirmation for SMS delivery", "status": "ready", "phase": "digital" },
                { "from": "appt-queue", "to": "receptionist", "text": "Appointment appears on receptionist dashboard", "status": "ready", "phase": "digital" },
                { "from": "online-portal", "to": "sms-gateway", "text": "Send instant booking confirmation SMS", "status": "ready", "phase": "digital", "popUp": { "type": "amplify", "msg": "Booked in 30 seconds — no phone call needed" } },
                { "from": "emr-db", "to": "reminder-fn", "text": "Trigger: 24-hour reminder scheduled", "status": "ready", "phase": "digital" },
                { "from": "reminder-fn", "to": "sms-gateway", "text": "\"Reminder: Bella's appointment is tomorrow at 10am\"", "status": "ready", "phase": "digital", "popUp": { "type": "amplify", "msg": "Automated reminders cut no-shows by 60%" } },
                { "from": "emr-db", "to": "vet-doctor", "text": "Dr. Chen reviews Bella's full digital history on tablet", "status": "ready", "phase": "digital" },
                { "from": "vet-doctor", "to": "exam-room", "text": "Examine Bella with complete medical history on screen", "status": "ready", "phase": "digital" }
            ]
        },
        {
            "id": "telehealth-consult",
            "name": "Phase 2 — Video Telehealth",
            "sequence": [
                { "from": "pet-owner", "to": "photo-upload", "text": "Upload photo: \"Bella's ear looks red and swollen\"", "status": "ready", "phase": "telehealth" },
                { "from": "photo-upload", "to": "emr-db", "text": "Attach photo to Bella's EMR record", "status": "ready", "phase": "telehealth" },
                { "from": "pet-owner", "to": "video-svc", "text": "Join video call from home — show Bella's ear on camera", "status": "ready", "phase": "telehealth" },
